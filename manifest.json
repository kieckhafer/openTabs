{
  "manifest_version": 3,

  "name": "openTabs",
  "description": "A tab organizer. openTabs gives you a count of your open tabs, and allows you to combine, extract, and sort tabs.",
  "version": "4.0.0",

  "icons": {
    "24": "icon24.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "permissions": [
    "tabs",
    "storage"
  ],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
